# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# API Keys (required)
VOYAGE_API_KEY=vo-xxxxxxxxxxxx
CEREBRAS_API_KEY=csk-xxxxxxxxxxxx
QDRANT_URL=https://your-cluster.cloud.qdrant.io
